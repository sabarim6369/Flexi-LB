{
  "name": "flexilb-e2e-testing",
  "version": "1.0.0",
  "description": "End-to-end testing suite for FlexiLB application using Selenium WebDriver",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "mocha tests/**/*.test.js --timeout 30000 --reporter spec",
    "test:login": "mocha tests/auth/login.test.js --timeout 30000 --reporter spec",
    "test:signup": "mocha tests/auth/signup.test.js --timeout 30000 --reporter spec",
    "test:auth": "mocha tests/auth/*.test.js --timeout 30000 --reporter spec",
    "test:headless": "HEADLESS=true mocha tests/**/*.test.js --timeout 30000 --reporter spec",
    "test:chrome": "BROWSER=chrome mocha tests/**/*.test.js --timeout 30000 --reporter spec",
    "test:firefox": "BROWSER=firefox mocha tests/**/*.test.js --timeout 30000 --reporter spec",
    "test:safari": "BROWSER=safari mocha tests/**/*.test.js --timeout 30000 --reporter spec",
    "test:report": "mocha tests/**/*.test.js --timeout 30000 --reporter mochawesome --reporter-options reportDir=reports,reportFilename=e2e-test-report",
    "install-drivers": "node utils/install-drivers.js",
    "clean": "rimraf reports/* && rimraf screenshots/*"
  },
  "keywords": [
    "selenium",
    "webdriver",
    "e2e-testing",
    "automation",
    "testing",
    "mocha",
    "flexilb"
  ],
  "author": "FlexiLB Team",
  "license": "MIT",
  "dependencies": {
    "selenium-webdriver": "^4.15.0",
    "mocha": "^10.2.0",
    "chai": "^4.3.10",
    "chromedriver": "^119.0.1",
    "geckodriver": "^4.2.1",
    "mochawesome": "^7.1.3",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "path": "^0.12.7",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.4",
    "@types/chai": "^4.3.9",
    "eslint": "^8.53.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sabarim6369/Flexi-LB.git"
  }
}